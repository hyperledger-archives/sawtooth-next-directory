/*******************************
        Global Overrides
*******************************/

body {
  -webkit-font-smoothing: antialiased;
}

body,
#root {
  height: 100%;
}

.clear {
  clear: both;
}

.pull-left {
  float: left;
}

.pull-right {
  float: right;
}

.dark {
  background: @darkBackground;
}
